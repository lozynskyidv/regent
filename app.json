{
  "expo": {
    "name": "WorthView",
    "slug": "regent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.dmy.networth",
      "buildNumber": "3",
      "infoPlist": {
        "NSFaceIDUsageDescription": "WorthView uses Face ID to securely access your financial data.",
        "UIRequiresFullScreen": true,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAFAFA"
      },
      "package": "com.dmy.networth",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "WorthView uses Face ID to securely access your financial data."
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "scheme": "worthview",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e2762de0-6530-42a2-903f-2a93705158f0"
      }
    },
    "owner": "lozynskyidv"
  }
}
